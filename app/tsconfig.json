{
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": ".",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@bitnation-dev/management/src/*": ["./node_modules/@bitnation-dev/management/dist/package/src/*"]
    }
  },
  "include": ["src/**/*", "./node_modules/@bitnation-dev/**/*.d.ts"]
}